<footer class="site-footer">
    <% if (config.rss || config.feed) {  %>
        <a class="subscribe icon-feed" href="<%= (config.rss || config.feed.path) %>"><span class="tooltip">Subscribe!</span></a>
    <% } %>

    <div class="inner">
        <div class="copyright">
            &copy;
            <%- (new Date()).getFullYear() %>
            <a href="<%- config.root %>">
                <%- config.title %>
            </a>
        </div>
        
        <% if (site.data.footer){ %>
            <% for (var i in site.data.footer){ %>
                <div class="<%- i %>">
                    <%- site.data.footer[i] %>
                </div>
            <% } %>
        <% } %>
    </div>
</footer>
